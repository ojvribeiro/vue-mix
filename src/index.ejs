<!doctype html>
<% if (typeof (lang) != 'undefined') { %><html lang="<%= lang %>"><% } else { %><html lang="en"><% } %>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <% if (typeof (head.title) != 'undefined') { %><title> <%= head.title %> </title><% } %>
    <% if (typeof (head.meta) != 'undefined') { %>
    <% head.meta.forEach(item => { %><meta <% if (typeof (item.charset) != 'undefined') { %>charset="<%= item.charset %>"<% } %><% if (typeof (item.property) != 'undefined') { %> property="<%= item.property %>"<% } %><% if (typeof (item.name) != 'undefined') { %> name="<%= item.name %>"<% } %><% if (typeof (item.content) != 'undefined') { %> content="<%= item.content %>"<% } %> />
    <% }) %><% } %>

    <style>
      html,body,body>[data-vulmix-app]{height: 100%;margin: 0;}
    </style>
  </head>

  <body>
    <div data-vulmix-app></div>

    <script src="<%= mix('/assets/_vulmix/js/manifest.js') %>"></script>
    <script src="<%= mix('/assets/_vulmix/js/vendor.js') %>"></script>
    <script src="<%= mix('/assets/_vulmix/js/main.vulmix.js') %>"></script>
  </body>
</html>
