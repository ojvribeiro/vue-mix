{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node", "webpack-env"],
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noImplicitAny": true,
    "outDir": "../../.vulmix/client",
    "baseUrl": "../.."
  },
  "include": [
    "../../**/*",
    "../../node_modules/vulmix/src/vue/**/*",
    "../../.vulmix/runtime/**/*",
    "components.d.ts",
    "vue-shims.d.ts",
    "auto-imports.d.ts"
  ],
  "exclude": ["../../_dist", "../../.vulmix/client", "../../node_modules"]
}
