{
  "compilerOptions": {
    "strict": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "Node",
    "types": ["node", "webpack-env"],
    "jsx": "preserve",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "outDir": "../../_dist",
    "baseUrl": "../..",
    "paths": {
      "~": ["./"]
    }
  },
  "include": [
    "../../../**/*",
    "components.d.ts",
    "vue-shims.d.ts",
    "auto-imports.d.ts"
  ],
  "exclude": ["../../_dist", "../../../utils", "../../../node_modules"]
}
